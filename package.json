{
  "name": "node-package-exports",
  "version": "1.1.0",
  "sideEffects": false,
  "description": "find node package entry or path in exports",
  "types": "./dist/index.d.ts",
  "main": "./dist/entry.cjs.js",
  "unpkg": "./dist/entry.umd.js",
  "module": "./dist/entry.esm-bundler.js",
  "exports": {
    ".": {
      "import": "./dist/entry.esm-bundler.js",
      "require": "./dist/entry.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "test": "jest",
    "build": "rimraf ./dist && rollup --config && rimraf ./dist/__tests__",
    "format": "prettier --write --parser typescript \"(src|dist)/**/*.(js|ts?(x))\""
  },
  "author": "imtaotao",
  "keywords": [
    "exports",
    "imports",
    "node",
    "node-package",
    "entry",
    "path",
    "require"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imtaotao/node-package-exports.git"
  },
  "bugs": {
    "url": "https://github.com/imtaotao/node-package-exports/issues"
  },
  "packageManager": "pnpm@9.1.3",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.0",
    "fs-extra": "^10.1.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "rimraf": "^5.0.7",
    "rollup": "^2.67.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "aidly": "^1.2.3"
  }
}
