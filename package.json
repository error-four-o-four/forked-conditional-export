    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.0",
    "fs-extra": "^10.1.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "rollup": "^2.67.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
